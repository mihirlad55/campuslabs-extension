{
  "name": "CampusLabs Assistant",
  "version": "0.0.0.1",
  "description": "Useful utilities for interacting with CampusLabs",
  "permissions": [
    "activeTab",
    "https://*.campuslabs.com/engage/submitter/organization/*/eventsubmission/create"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "CampusLabs Assistant",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/logo-16.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  },
  "manifest_version": 2
}
